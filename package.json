{
  "name": "exchange-booth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/client/main.ts",
    "clean": "rm -rf dist",
    "deploy": "solana program deploy dist/program/exchange_booth.so",
    "generate:keys": "solana-keygen new -o dist/keys/mint1.so --no-bip39-passphrase --force; solana-keygen new -o dist/keys/mint2.so --no-bip39-passphrase --force;",
    "build:program": "cargo build-bpf --manifest-path=./src/program/Cargo.toml --bpf-out-dir=dist/program",
    "rebuild": "npm run clean && npm run build:program && npm run generate:keys && npm run deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@solana/spl-token": "^0.2.0",
    "@solana/web3.js": "^1.37.0",
    "borsh": "^0.7.0",
    "mz": "^2.7.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^5.1.0",
    "@types/eslint": "^8.2.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/mz": "^2.7.2",
    "@types/prettier": "^2.1.5",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "",
  "license": "ISC"
}
